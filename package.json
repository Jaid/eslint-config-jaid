{
  "name": "eslint-config-jaid",
  "version": "21.5.0",
  "type": "module",
  "description": "Personal ESLint preset. Intentionally ugly to write code that looks like I feel.",
  "keywords": [
    "ecmascript",
    "eslint",
    "eslint-config",
    "eslint-configs",
    "eslint-preset",
    "eslint-shareable-configs",
    "eslintconfig",
    "format",
    "formatter",
    "formatting",
    "jaid",
    "javascript",
    "jsx",
    "lint",
    "linter",
    "react",
    "style",
    "style-guide"
  ],
  "author": "Jaid <jaid.jsx@gmail.com> (https://github.com/Jaid)",
  "funding": "https://github.com/sponsors/jaid",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jaid/eslint-config-jaid"
  },
  "scripts": {
    "build": "bun x rollup --config",
    "build:production": "NODE_ENV=production bun run build",
    "buildPage": "fse emptyDir dist/page && bun src/scripts/activeHtml.ts && bun src/scripts/unusedHtml.ts && bun src/scripts/deprecatedHtml.ts && fse copy etc/page dist/page",
    "inspectConfig": "bun --bun x @eslint/config-inspector --config src/index.ts",
    "inspectStatic": "bun run inspectConfig build",
    "printConfig": "bun x eslint --print-config src/index.ts",
    "release": "fd --unrestricted --full-path 'dist[\\\\/]package[\\\\/][\\w-]+[\\\\/]production[\\\\/]package.json' --exec bun publish --cwd {//}",
    "test": "bun test ./test",
    "tsc": "bun x tsc"
  },
  "dependencies": {
    "@stylistic/eslint-plugin": "^5.6.1",
    "@typescript-eslint/eslint-plugin": "8.49.0",
    "@typescript-eslint/parser": "8.49.0",
    "browserslist-config-jaid-node": "3.0.0",
    "eslint": "^9.39.1",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import-quotes": "^0.0.1",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-jsonc": "^2.21.0",
    "eslint-plugin-n": "^17.23.1",
    "eslint-plugin-perfectionist": "^4.15.1",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-regexp": "^2.10.0",
    "eslint-plugin-unicorn": "^62.0.0",
    "eslint-plugin-yml": "^1.19.0",
    "eslint-plugin-you-dont-need-lodash-underscore": "^6.14.0",
    "forward-slash-path": "^0.3.0",
    "jsonc-eslint-parser": "^2.4.2",
    "lodash-es": "^4.17.21",
    "yaml-eslint-parser": "^1.3.2"
  },
  "devDependencies": {
    "@atao60/fse-cli": "^0.1.9",
    "@eslint/config-inspector": "^1.4.2",
    "@types/node": "^24.10.2",
    "chalk": "^5.6.2",
    "debug": "^4.4.3",
    "dts-bundle-generator": "^9.5.1",
    "emp": "^2.0.0",
    "eslint-config-jaid": "^21.5.0",
    "filter-obj": "^6.1.0",
    "fs-extra": "^11.3.2",
    "globby": "^16.0.0",
    "handlebars": "^4.7.8",
    "immer": "^11.0.1",
    "js-yaml": "^4.1.1",
    "list-dir-content-size": "^0.0.5",
    "more-types": "^0.6.0",
    "pretty-bytes": "^7.1.0",
    "publishimo": "^5.0.6",
    "read-file-yaml": "^2.0.0",
    "rollup": "^4.53.3",
    "rollup-config-factory": "^0.4.2",
    "showdown": "^2.1.0",
    "sort-keys": "^6.0.0",
    "tsconfig-jaid-node": "^9.0.0",
    "tsx": "^4.21.0",
    "type-fest": "^5.3.1",
    "typescript": "^5.9.3",
    "wireit": "^0.14.12",
    "zeug": "^2.1.0"
  }
}