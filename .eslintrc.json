{
  "extends": "jaid",
  "overrides": [
    {
      "files": [
        "src/rules/**/*.yml"
      ],
      "rules": {
        "yml/sort-keys": ["warn", [
          {
            "pathPattern": "^rules$",
            "order": {
              "natural": true
            }
          }
        ]]
      }
    }
  ]
}
